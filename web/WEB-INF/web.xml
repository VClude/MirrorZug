        <servlet-name>verifypassword</servlet-name>
        <url-pattern>/verifypassword/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateuserdata</servlet-name>
        <url-pattern>/updateuserdata/*</url-pattern>
    </servlet-mapping>
